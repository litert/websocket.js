    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "[LiteRT/WebSocket] Start HTTP Server",
            "program": "${workspaceFolder}/src/examples/http/server.ts",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "[LiteRT/WebSocket] Start HTTP Client",
            "program": "${workspaceFolder}/src/examples/http/client.ts",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "[LiteRT/WebSocket] Start HTTPS Server",
            "program": "${workspaceFolder}/src/examples/https/server.ts",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "[LiteRT/WebSocket] Start HTTPS Client",
            "program": "${workspaceFolder}/src/examples/https/client.ts",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}